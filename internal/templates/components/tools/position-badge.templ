package tools

import "github.com/knackwurstking/pgpress/internal/database"

templ PositionBadge(position database.Position) {
	switch position {
		case database.PositionTop:
			<small class="badge badge-sm badge-outline">
				Oberteil
			</small>
		case database.PositionBottom:
			<small class="badge badge-sm badge-outline">
				Unterteil
			</small>
	}
}
